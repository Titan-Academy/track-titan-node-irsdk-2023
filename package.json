{
  "name": "@titan-academy/track-titan-node-irsdk-2023",
  "version": "0.0.8",
  "description": "iRacing SDK implementation for nodejs",
  "main": "src/node-irsdk.js",
  "binary": {
    "module_name": "track_titan_node_irsdk",
    "module_path": "./lib/binding/napi-v{napi_build_version}",
    "remote_path": "./{module_name}/v{version}/{configuration}/",
    "package_name": "{platform}-{arch}-napi-v{napi_build_version}.tar.gz",
    "host": "https://your_bucket.s3-us-west-1.amazonaws.com",
    "napi_versions": [
      8,
      9
    ]
  },
  "scripts": {
    "installFromSource": "npm install --build-from-source",
    "install": "node-pre-gyp install",
    "prebuild": "node-pre-gyp configure build",
    "publish": "node-pre-gyp package publish",
    "testbinary": "node-pre-gyp testpackage"
  },
  "keywords": [
    "irsdk",
    "iRacing SDK",
    "iRacing"
  ],
  "author": "Holger Miefert",
  "license": "MIT",
  "dependencies": {
    "node-addon-api": "*",
    "@mapbox/node-pre-gyp": "^1.0.11",
    "js-yaml": "^4.1.0",
    "nan": "^2.17.0"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/@titan-academy"
  },
  "devDependencies": {
    "aws-sdk": "^2.1692.0"
  }
}